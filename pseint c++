int main() {
	float a;
	float b;
	float c;
	float d;
	float e;
	float f;
	float g;
	cout << "ingrese el primer numero:" << endl;
	cin >> a;
	cout << "ingrese el segundo numero:" << endl;
	cin >> b;
	c = a+b;
	d = a-b;
	e = a*b;
	f = a/b;
	g = a%b;
	cout << "la suma es:" << c << endl;
	cout << "la resta es:" << d << endl;
	cout << "el producto es:" << e << endl;
	cout << "la division es:" << f << endl;
	cout << "el modulo es:" << g << endl;
